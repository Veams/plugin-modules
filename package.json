{
	"name": "veams-plugin-modules",
	"version": "1.0.6",
	"description": "The VeamsModules plugin provides a whole system to initialize, render, save and destroy your modules.",
	"main": "lib/index.js",
	"browserify": {
		"transform": [
			"babelify"
		]
	},
	"scripts": {
		"build": "webpack --mode=dev && webpack --mode=build",
