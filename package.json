		"babel-preset-es2015": "^6.9.0",
		"babel-preset-stage-0": "^6.16.0",
		"babelify": "^7.3.0",
		"chai": "3.4.1",
		"eslint": "1.7.2",
		"eslint-loader": "1.1.0",
		"mocha": "2.3.4",
		"webpack": "1.12.9",
		"yargs": "3.32.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/veams/veams-plugin-modules.git"
	},
